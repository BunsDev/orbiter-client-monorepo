{
  "extends": ["../../.eslintrc.json"],
  "ignorePatterns": ["!**/*"],
  "plugins": [
    "unused-imports",
    "simple-import-sort"
  ],
  "overrides": [
    {
      "files": ["*.ts", "*.tsx", "*.js", "*.jsx"],
      "rules": {}
    },
    {
      "files": ["*.ts", "*.tsx"],
      "rules": {
        "@typescript-eslint/no-empty-interface":"off",
        "@typescript-eslint/no-empty-function":"off"
      }
    },
    {
      "files": ["*.js", "*.jsx"],
      "rules": {}
    }
  ]
}
